# 작은것을 석택하여 제일 앞으로 보내는 알고리즘
a = [1, 10, 5, 8, 7, 6, 4, 3, 2, 9]

min = 0
index = 0
temp = 0

print (a)

for i in range (10):
    min = 9999
    for j in range (i, 10):
        if min > a[j]:
            min = a[j]
            index = j

    temp = a[i]
    a[i] = a [index]
    a[index] = temp

print (a)
